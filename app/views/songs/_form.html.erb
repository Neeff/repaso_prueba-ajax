<%= form_with(model: song) do |f| %>
        <%= f.label :Title%>
        <%= f.text_field :title %><br>
        <%= f.label :Artist %>
        <%= f.text_field :artist %><br>
        <%= f.label :Year %>
        <%= f.text_field :year %><br>
        <%= f.submit class: 'btn btn-success' %>
<% end %>